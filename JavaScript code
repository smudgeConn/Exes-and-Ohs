function XO(str) {
  console.log(str.toLowerCase());
  let array = str.toLowerCase().split("");
  console.log(array);
  let x = 0;
  let o = 0;
  
  for (let i = 0; i < array.length; i++) {
    if(array[i] == 'x') {
      x++;
    } else if(array[i] == 'o') {
      o++;
    }
  }
  
  if(o==x){return true}
  else {return false}
  }
